<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:height="20dp"
                android:width="20dp" />
            <solid android:color="#85AFD6" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="oval">
            <size
                android:width="19dp"
                android:height="19dp" />
            <solid android:color="#85AFD6" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="3dp"
        android:right="2dp"
        android:top="4dp"
        android:drawable="@drawable/check"
        android:width="20dp"
        android:height="20dp">

    </item>
</layer-list>
